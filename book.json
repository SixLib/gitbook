{
  "title": "sixlib的小书",
  "language": "zh-hans",
  "author": "sixlib",
  "plugins": [
    "-sharing",
    "-lunr",
    "-search",
    "splitter",
    "search-pro-kui",
    "toggle-chapters",
    "anchor-navigation-ex",
    "github",
    "tbfed-pagefooter",
    "echarts",
    "new-flowchart",
    "include-csv",
    "alerts"
  ],
  "pluginsConfig": {
    "chart": {},
    "theme-default": {
      "showLevel": true
    },
    "anchor-navigation-ex": {
	    "float": { 
	        "showLevelIcon": true,
	        "level1Icon": "fa fa-hand-o-right",
	        "level2Icon": "fa fa-hand-o-right",
	        "level3Icon": "fa fa-hand-o-right"
	    }
	},
    "sectionx": {
      "tag": "b"
    },
    "github": {
      "url": "https://github.com/sixlib/gitbook"
    },
    "tbfed-pagefooter": {
      "copyright": "Copyright © sixlib的小书 2018",
      "modify_label": "该文件修订时间：",
      "modify_format": "YYYY-MM-DD HH:mm:ss"
    }
  }
}
