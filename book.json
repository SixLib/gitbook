{
  "title": "sixlib的小书",
  "language": "zh-hans",
  "author": "sixlib",
  "plugins": [
    "-sharing",
    "-lunr",
    "-search",
    "splitter",
    "search-pro-kui",
    "toggle-chapters",
    "back-to-top-button",
    "sectionx",
    "github",
    "tbfed-pagefooter",
